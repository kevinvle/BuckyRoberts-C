#include <stdio.h>
#include <stdlib.h>



int main()
{
    int age;
    char gender;

    printf("How old are you? \n");
    scanf(" %d", &age);

    printf("What is your gender? (m/f) \n");
    scanf(" %c", &gender);

    if (age >= 18) {
        printf("you may enter this wwebsite!");

        if (gender == 'm') {
            printf("dude");
        }

        if (gender == 'f') {
            printf("maam");
        }
    }

    if (age < 18) {
        printf("nothing to see here");
    }

    return 0;
}



